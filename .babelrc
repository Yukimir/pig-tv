{
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": false,
                "moduleName": "babel-runtime"
            }
        ]
    ],
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browserslist": [
                        "not ie <= 8"
                    ]
                }
            }
        ]
    ]
}